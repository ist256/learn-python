   "source": [
    "# Watch Me Code 3: Count The I's\n",
    "\n",
    "This program will count the number of i's in the input word.\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter a word: mississippi\n",
      "There are 4 i's in the word mississippi\n"
     ]
    }
   ],
   "source": [
    "word = input(\"Enter a word: \")\n",
    "count = 0\n",
    "for letter in word:\n",
    "    if letter== 'i':\n",
    "        count = count + 1\n",
    "\n",
    "print(\"There are %d i's in the word %s\" % (count,word))"
   ]
